   "cell_type": "code",
   "execution_count": 4,
   "id": "80b8fd8d",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-04-05T00:03:08.692580Z",
     "iopub.status.busy": "2025-04-05T00:03:08.692371Z",
     "iopub.status.idle": "2025-04-05T00:03:08.893658Z",
     "shell.execute_reply": "2025-04-05T00:03:08.892992Z"
    },
    "papermill": {
     "duration": 0.205417,
     "end_time": "2025-04-05T00:03:08.895051",
     "exception": false,
     "start_time": "2025-04-05T00:03:08.689634",
     "status": "completed"
    },
